<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.flickipedia.fxml.InsertMovieFXMLController">
   <children>
      <Label text=" " />
      <Label text="Insert New Movie">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Label text=" " />
      <HBox alignment="CENTER" prefHeight="16.0" prefWidth="400.0">
         <children>
            <Label text="Id Number " />
            <TextField fx:id="idField" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="16.0" prefWidth="400.0">
         <children>
            <Label text="Name " />
            <TextField fx:id="nameField" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="16.0" prefWidth="400.0">
         <children>
            <Label text="Country " />
            <TextField fx:id="countryField" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="16.0" prefWidth="400.0">
         <children>
            <Label text="Age Rating " />
            <TextField fx:id="ageRatingField" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="16.0" prefWidth="400.0">
         <children>
            <Label text="Day " />
            <TextField fx:id="dayField" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="16.0" prefWidth="400.0">
         <children>
            <Label text="Month " />
            <TextField fx:id="monthField" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="16.0" prefWidth="400.0">
         <children>
            <Label text="Year " />
            <TextField fx:id="yearField" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="16.0" prefWidth="400.0">
         <children>
            <Label text="Duration " />
            <TextField fx:id="durationField" />
         </children>
      </HBox>
      <Label text=" " />
      <HBox alignment="TOP_CENTER" prefHeight="21.0" prefWidth="400.0">
         <children>
            <Button fx:id="insertBtn" mnemonicParsing="false" onAction="#insertBtnPressed" text="Insert" />
         </children>
      </HBox>
      <Label text=" " />
      <Label fx:id="errorLbl" text="Invalid XXXXXXXXXX Field!" textFill="RED">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</VBox>
